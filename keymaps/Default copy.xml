<keymap version="1" name="Default copy" parent="$default">
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="alt z" />
  </action>
  <action id="Console.TableResult.EditFilterCriteria" />
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="alt x" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl alt l" />
    <keyboard-shortcut first-keystroke="shift ctrl alt f" />
  </action>
</keymap>